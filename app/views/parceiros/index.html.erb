<%= link_to "Adicionar Parceiros", new_parceiro_path, class: "btn btn-primary btn-lg "%>

 <div class="box-produtos">
            <div class="box-header">
              <h3 class="box-title">Parceiros</h3>

              <div class="box-tools">
                <div class="input-group input-group-sm" style="width: 150px;">
                  <input type="text" name="table_search" class="form-control pull-right" placeholder="Search">

                  <div class="input-group-btn">
                    <button type="submit" class="btn btn-default"><i class="fa fa-search"></i></button>
                  </div>
                </div>
              </div>
            </div>
            <!-- /.box-header -->
            <div class="box-body table-responsive no-padding">
              <table class="table table-hover">

                    <tr>
                      <th>Nome</th>
                      <th>Cpf cnpj</th>
                      <th>Rg</th>
                      <th>Telefone</th>
                      <th>Apelido</th>
                      <th>Fisica juridica</th>
                      <th>Dt nacimento</th>
                      <th>Cliente</th>
                      <th>Fornecedor</th>
                      <th>Cep</th>
                      <th>Endereco</th>
                      <th>Numero</th>
                      <th>Ibge</th>
                      <th>Bairro</th>
                      <th>Cidade</th>
                      <th>Uf</th>
                      <th>Pais</th>
                      <th colspan="3"></th>
                    </tr>
                  </thead>

                 
                <% todos = Parceiro.all  %>
                <% todos.each do |parceiro| %>
                      <tr>
                        <td><%= parceiro.nome %></td>
                        <td><%= parceiro.cpf_cnpj %></td>
                        <td><%= parceiro.rg %></td>
                        <td><%= parceiro.telefone %></td>
                        <td><%= parceiro.apelido %></td>
                        <td><%= parceiro.fisica %></td>
                        <td><%= parceiro.juridica%></td>
                        <td><%= parceiro.dt_nacimento %></td>
                        <td><%= parceiro.cliente %></td>
                        <td><%= parceiro.fornecedor %></td>
                        <td><%= parceiro.cep %></td>
                        <td><%= parceiro.endereco %></td>
                        <td><%= parceiro.numero %></td>
                        <td><%= parceiro.ibge %></td>
                        <td><%= parceiro.bairro %></td>
                        <td><%= parceiro.cidade %></td>
                        <td><%= parceiro.uf %></td>
                        <td><%= parceiro.pais %></td>
                        <td><%= link_to 'Edit', edit_parceiro_path(parceiro) %></td>
                        <td><%= link_to 'Excluir', parceiro, data: { confirm: 'Are you sure?' } %></td> 
                      </tr>
                
 
        </tr>
           <%end%>
                </table>
          
            <!-- /.box-body -->
          </div>
          </div>
       