<p id="notice"><%= notice %></p>

<h1>Listing Produto Chaves</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @produto_chaves.each do |produto_chafe| %>
      <tr>
        <td><%= link_to 'Show', produto_chafe %></td>
        <td><%= link_to 'Edit', edit_produto_chafe_path(produto_chafe) %></td>
        <td><%= link_to 'Destroy', produto_chafe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

